declare module "aphrodite" {
    declare export type Extension = {
        selectorHandler: (string) => ?Object;
    };

    declare export type SheetDeclaration = {
        [key: string]: [string, Object];
    };

    declare export type StyleSheet = {
        create: (sheets: Object) => SheetDeclaration;
        extend: (extensions: Extension[]) => StyleSheet;
    };

    declare export function css(sheet: [string, Object]): string;
}
