{
  "private": true,
  "name": "aesthetic",
  "version": "0.0.1",
  "description": "Abstract library to support a range of styling options for React components.",
  "keywords": [],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "rimraf ./packages/**/lib",
    "build": "lerna run build",
    "setup": "npm run bootstrap && npm run clean && npm run build",
    "docs": "cp ./README.md ./packages/aesthetic/README.md",
    "flow": "flow check",
    "lint": "eslint './packages/**/{src,tests}/*.js'",
    "mocha": "mocha --opts ./mocha.opts './packages/**/tests/*.test.js'",
    "pretest": "npm run setup",
    "test": "npm run mocha",
    "posttest": "npm run lint && npm run flow",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/aesthetic.git"
  },
  "author": {
    "name": "Miles Johnson",
