  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/aesthetic/issues"
  },
  "homepage": "https://github.com/milesj/aesthetic#readme",
  "devDependencies": {
    "aphrodite": "^1.1.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
