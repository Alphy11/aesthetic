    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.37.4",
    "glamor": "^2.20.12",
    "jsdom": "^9.9.1",
    "jss": "^6.0.1",
    "lerna": "2.0.0-beta.31",
    "mocha": "^3.1.2",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.4.0",
